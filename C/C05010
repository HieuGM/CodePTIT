#include<stdio.h>
#include<math.h>
int snt(int n)
{
	int a = sqrt(n);
	for (int i = 2; i <= a; i++)
	{
		if (n%i==0) return 0;
	}
	return n>1;
}
int main()
{
	int n;
	scanf("%d", &n);
	int a[55][55];
	for (int i = 0; i < n; i++)
	{
		for (int j = 0; j<n; j++) scanf("%d", &a[i][j]);
	}
	int sum = 0;
	for (int i = 0; i < n; i++)
	{
		for (int j = i; j < n; j++)
		{
			if (snt(a[i][j])) sum += a[i][j];
		}
	}
	printf("%d", sum);
}
